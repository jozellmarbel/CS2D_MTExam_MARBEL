age = int(input("Enter your age: "))

if age < 5:
  print("You are a Kid.")
else:
  if age <= 12:
    print("You are a Youth.")
  else:
    if age <= 19:
      print("You are a Teenager.")
    else:
      if age <= 35:
        print("You are a Adolescent.")
      else:
        if age <= 60:
          print("You are Elderly.")
        else:
          print("You are a Senior.")
